{
  "name": "qgrep-code",
  "displayName": "qgrep-code",
  "description": "",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/NightRa/qgrep-vscode"
  },
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:qgrep-code.loadQGrepProject"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "qgrep-code.loadQGrepProject",
        "title": "QGrep: Load project"
      }
    ],
    "configuration": {
      "title": "QGrep",
      "properties": {
        "qgrep.binPath": {
          "type": "string",
          "default": "",
          "description": "Path to the qgrep executable"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "npm-qgrep": "github:NightRa/npm-qgrep#671b3b7e5b6258e11fab858bea2877e8ae528635",
    "vscode-regexpp": "^3.1.0"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  }
}
